  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "graph",
      "title": "Training Loss",
      "targets": [
        {
          "expr": "training_loss",
          "legendFormat": "Training Loss"
        }
