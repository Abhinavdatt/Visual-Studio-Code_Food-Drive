        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "graph",
      "title": "Validation Accuracy",
      "targets": [
        {
          "expr": "validation_accuracy",
          "legendFormat": "Validation Accuracy"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "graph",
      "title": "Prediction v1 Accuracy",
      "targets": [
        {
          "expr": "prediction_v1_accuracy",
          "legendFormat": "Prediction v1 Accuracy"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      }
